WHITESPACE = _{ " " }

hrs = { ('0'..'2' ~ '0'..'9') | ('0'..'9') }
min = { ('0'..'5' ~ '0'..'9') | ('0'..'9') }
ms = { ('0'..'9' ~ '0'..'9' ~ '0'..'9') | ('0'..'9' ~ '0'..'9') | ('0'..'9') }

duration = { hrs ~ ":" ~ min ~ ":" ~ min ~ "." ~ ms }

var_decl = { "let" ~ (('a'..'z')|('A'..'Z')|('0'..'9'))+ ~ "=" ~ duration }

